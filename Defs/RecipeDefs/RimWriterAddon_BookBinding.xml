<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <RecipeDef ParentName="MakeHentaiByHand" Name="MakeHentaiAtDesk">
        <defName>RimWriter_MakeHentaiAtTable</defName>
        <label>draw a hentai at a desk</label>
        <description>Makes a hentai comic at a writing table by inking out on flattened stationery such as paper or parchment and binding together the pieces.</description>
        <jobString>Drawing hentai.</jobString>
        <workAmount>15000</workAmount>
        <recipeUsers Inherit="False">
            <li>RimWriter_TableWriting</li>
        </recipeUsers>
        <products Inherit="False">
            <RimWriter_Hentai>1</RimWriter_Hentai>
        </products>
    </RecipeDef>

    <RecipeDef Name="MakeHentaiByHand">
        <defName>RimWriter_MakeHentai</defName>
        <label>draw a hentai by hand</label>
        <description>Makes a hentai by hand at a crafting spot by drawing out on flattened stationery such as paper or parchment and binding together the pieces.</description>
        <jobString>Drawing hentai by hand.</jobString>
        <workAmount>20000</workAmount>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
        <workSkill>Artistic</workSkill>
        <unfinishedThingDef>UnfinishedWriting</unfinishedThingDef>
        <soundWorking>RimWriter_SoundManualPencil</soundWorking>
        <effectWorking>Tailor</effectWorking>
        <allowMixingIngredients>true</allowMixingIngredients>
        <recipeUsers>
            <li>CraftingSpot</li>
        </recipeUsers>
        <products>
            <RimWriter_Hentai>1</RimWriter_Hentai>
        </products>
        <ingredients>
            <li>
                <filter>                
                    <categories>
                        <li>Leathers</li>
                    </categories>
                </filter>
                <count>10</count>
            </li>
            <li>
                <filter>
                    <categories>
                        <li>RimWriter_BookMaterials</li>
                    </categories>
                </filter>
                <count>100</count>
            </li>
        </ingredients>
        <fixedIngredientFilter>
            <categories>
                <li>RimWriter_BookMaterials</li>
                <li>Leathers</li>
            </categories>
        </fixedIngredientFilter>
    </RecipeDef>
	
